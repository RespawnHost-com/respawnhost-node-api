{
  "name": "@respawnhost/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for RespawnHost.com game server hosting API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && bun build ./index.ts --outdir ./dist --target node --format esm",
    "dev": "bun run index.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "respawnhost",
    "game-server",
    "api",
    "sdk",
    "typescript",
    "minecraft",
    "hosting"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/respawnhost-ts-sdk"
  },
  "bugs": {
    "url": "https://github.com/your-username/respawnhost-ts-sdk/issues"
  },
  "homepage": "https://github.com/your-username/respawnhost-ts-sdk#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
